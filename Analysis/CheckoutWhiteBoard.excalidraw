    {
      "id": "cshH_lAjD46zCTzT2g62S",
      "type": "text",
      "x": 575,
      "y": 1017,
      "width": 911.6666870117188,
      "height": 126.54770848985714,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
      "roughness": 2,
      "opacity": 100,
      "groupIds": [],
